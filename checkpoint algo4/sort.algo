ALGORITHM Insertion_sort
VAR
 tab: ARRAY_OF INTEGER[4]
 n : INTEGER;
 m; INTEGER
BEGIN
  PROCEDURE Sort(arr,n)
  VAR
      i, key, j :INTEGER
  BEGIN
      FOR i FROM 0 TO n-1  DO
          j=i - 1;
          WHILE (j>=0 && arr[j]> key) DO
              arr[j + 1] := arr[j];
              j:= j - 1;
          END_WHILE
          arr[j + 1] = key;
      END_FOR
END